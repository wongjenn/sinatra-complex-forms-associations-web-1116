<h1>Create a new Pet</h1>
<form action="/pets" method="POST">
  <label>Pet Name:</label>

  <br></br>

  <input id="pet_name" type="text" name="pet[name]">

  <br></br>
  <%Owner.all.each do |owner|%>
    <p><input type="checkbox" name="pet[owner_id]" value="<%=owner.id%>" id="<%=owner.id%>"><%=owner.name%></input></p>
    <%end%>

    <br></br>
    <label for="">Create New Owner:</label>
    <input id="owner_name" type="text" name="owner[name]" value="">
    <br></br>
  <input type="submit" value="Create Pet">
</form>
